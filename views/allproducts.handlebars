<div class="row">

  {{#each products}}
  <div class="col">
    <div class="card" style="width: 18rem;">
      <img src={{image}} class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title">{{product_name}}</h5>
        <p class="card-text">{{product_type }}</p>
        <a class="card-text">${{price}}</a>
      </div>
    </div>
  </div>

  {{/each}}

